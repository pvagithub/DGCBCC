@using System.Configuration

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Admin</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/Areas/Admin/Content/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- MetisMenu CSS -->
    <link href="~/Areas/Admin/Content/bower_components/metisMenu/dist/metisMenu.min.css" rel="stylesheet">
    <!-- Timeline CSS -->
    <link href="~/Areas/Admin/Content/dist/css/timeline.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/Areas/Admin/Content/dist/css/sb-admin-2.css" rel="stylesheet">
    <!-- Morris Charts CSS -->
    <link href="~/Areas/Admin/Content/bower_components/morrisjs/morris.css" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="~/Areas/Admin/Content/bower_components/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Scripts/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

    <script src="~/Areas/Admin/Content/bower_components/jquery/dist/jquery.min.js"></script>
    <link href="~/Content/Site2.css" rel="stylesheet" />
    <link href="~/Scripts/StickyTableHeaders/css/component2.css" rel="stylesheet" />
    @*<link href="~/Scripts/StickyTableHeaders/css/demo.css" rel="stylesheet" />*@
    <link href="~/Scripts/StickyTableHeaders/css/normalize.css" rel="stylesheet" />
    <script src="~/Scripts/StickyTableHeaders/js/jquery.ba-throttle-debounce.min.js"></script>
    <script src="~/Scripts/StickyTableHeaders/js/jquery.stickyheader.js"></script>
    <style>
        .se-pre-con {
            background: #fff url("../Images/IconWeb/Preloader_2.gif") no-repeat scroll center center;
            height: 100%;
            left: 0;
            opacity: 0.8;
            position: fixed;
            top: 0;
            width: 100%;
            z-index: 9999;
            display: none;
        }
    </style>
    <script>
        $(document).ready(function () {
            $('.parent').on('click', function () {
                var data = $(this).attr("data-value");
                $('tr.child').each(function () {
                    if ($(this).attr("data-value") == data) {
                        $(this).toggle();
                    }
                    else {
                        $(this).css("display", "none");
                    }
                });
            });
        })
    </script>

</head>
<body>
    <div class="se-pre-con"></div>
    <div id="wrapper">

        @Html.Partial("_Header")

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <!-- Bootstrap Core JavaScript -->
    <script src="~/Areas/Admin/Content/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/Areas/Admin/Content/bower_components/metisMenu/dist/metisMenu.min.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="~/Areas/Admin/Content/bower_components/raphael/raphael-min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/Areas/Admin/Content/dist/js/sb-admin-2.js"></script>
    <script src="~/Scripts/bootbox.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>